    {
      "name": "Cliente",
      "description": "Operaciones disponibles para CLIENTE: crear solicitudes, consultar el detalle y hacer seguimiento del contenedor.",
      "auth": {
        "type": "bearer",
        "bearer": [
          {
            "key": "token",
            "value": "{{token_cliente}}",
            "type": "string"
          }
        ]
      },
      "item": [
        {
          "name": "1. CLIENTE - Crear solicitud de transporte",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"cliente\": {\n    \"nombre\": \"ACME Corp\",\n    \"email\": \"contacto@acme.com\",\n    \"telefono\": \"+54 11 5555-1111\",\n    \"cuit\": \"30-12345678-9\"\n  },\n  \"contenedor\": {\n    \"codigo\": \"CONT-XYZ-1234\",\n    \"peso\": 1200.5,\n    \"volumen\": 28.4\n  },\n  \"origen\": {\n    \"direccion\": \"Buenos Aires\",\n    \"latitud\": -34.6037,\n    \"longitud\": -58.3816\n  },\n  \"destino\": {\n    \"direccion\": \"Córdoba\",\n    \"latitud\": -31.4201,\n    \"longitud\": -64.1888\n  }\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{base_url_gateway}}/orders",
              "host": [
                "{{base_url_gateway}}"
              ],
              "path": [
                "orders"
              ]
            },
            "description": "Crea una solicitud completa registrando cliente y contenedor si no existían. Guarda el `id` devuelto en `{{solicitud_id}}` y el contenedor en `{{contenedor_id}}`."
          }
        },
        {
          "name": "1. CLIENTE - Consultar solicitud por ID",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url_gateway}}/orders/{{solicitud_id}}",
              "host": [
                "{{base_url_gateway}}"
              ],
              "path": [
                "orders",
                "{{solicitud_id}}"
              ]
            },
            "description": "Devuelve el detalle y estado actual de la solicitud creada."
          }
        },
        {
          "name": "1. CLIENTE - Tracking resumido de solicitud",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url_gateway}}/orders/{{solicitud_id}}/tracking",
              "host": [
                "{{base_url_gateway}}"
              ],
              "path": [
                "orders",
                "{{solicitud_id}}",
                "tracking"
              ]
            },
            "description": "Permite al cliente revisar el estado del contenedor asociado y el resumen de ruta."
          }
        },
        {
          "name": "1. CLIENTE - Seguimiento detallado de contenedor",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url_gateway}}/logistics/seguimiento/contenedores/{{contenedor_id}}",
              "host": [
                "{{base_url_gateway}}"
              ],
              "path": [
                "logistics",
                "seguimiento",
                "contenedores",
                "{{contenedor_id}}"
              ],
              "query": [
                {
                  "key": "solicitudId",
                  "value": "{{solicitud_id}}",
                  "description": "Solicitud a la que pertenece el contenedor"
                }
              ]
            },
            "description": "Consulta extendida de Logistics para ver tramo actual, posiciones y próximos pasos del contenedor."
          }
        }
      ]
    },
    {
      "name": "Operador - Solicitudes",
      "description": "Endpoints del micro Orders usados por el OPERADOR para validar costos y estados.",
      "auth": {
