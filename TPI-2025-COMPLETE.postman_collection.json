            "method": "GET",
            "url": {
              "raw": "{{base_url_gateway}}/orders/{{solicitud_id}}",
              "host": [
                "{{base_url_gateway}}"
              ],
              "path": [
                "orders",
                "{{solicitud_id}}"
              ]
            },
            "description": "Revisión previa de la solicitud antes de planificar logística."
          }
        },
        {
          "name": "2. OPERADOR - Calcular estimación de costos y tiempos",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"combustiblePrecioLitro\": 750,\n  \"tiempoCargaHoras\": 4\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{base_url_gateway}}/orders/{{solicitud_id}}/estimacion",
              "host": [
                "{{base_url_gateway}}"
              ],
              "path": [
                "orders",
                "{{solicitud_id}}",
                "estimacion"
              ]
            },
            "description": "Invoca el cálculo integrado con distance-client y métricas de flota."
          }
        },
        {
          "name": "2. OPERADOR - Actualizar costo final de solicitud",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"costoFinal\": 185000,\n  \"tiempoRealMinutos\": 810\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{base_url_gateway}}/orders/{{solicitud_id}}/costo",
              "host": [
                "{{base_url_gateway}}"
              ],
              "path": [
                "orders",
                "{{solicitud_id}}",
                "costo"
              ]
            },
            "description": "Cierra la solicitud cuando Logistics informa valores finales."
          }
        },
        {
          "name": "2. OPERADOR - Contenedores pendientes en Orders",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url_gateway}}/orders/containers/pendientes",
              "host": [
                "{{base_url_gateway}}"
              ],
              "path": [
                "orders",
                "containers",
                "pendientes"
              ],
              "query": [
                {
                  "key": "estado",
                  "value": "",
                  "description": "Filtrar por estado logístico"
                },
                {
                  "key": "depositoId",
                  "value": "{{deposito_id}}",
                  "description": "Filtrar por depósito"
                }
              ]
            },
            "description": "Vista de Orders para identificar contenedores que aún requieren planificación logística."
          }
        }
      ]
    },
    {
      "name": "Operador - Logística",
      "description": "Planificación de rutas, seguimiento operativo y asignación de camiones realizada por el OPERADOR.",
      "auth": {
        "type": "bearer",
        "bearer": [
          {
            "key": "token",
            "value": "{{token_operador}}",
            "type": "string"
          }
        ]
      },
      "item": [
        {
          "name": "3. OPERADOR - Generar rutas tentativas",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url_gateway}}/logistics/routes/solicitudes/{{solicitud_id}}/rutas-tentativas",
              "host": [
                "{{base_url_gateway}}"
              ],
              "path": [
                "logistics",
                "routes",
                "solicitudes",
                "{{solicitud_id}}",
                "rutas-tentativas"
              ]
            },
            "description": "Calcula todas las alternativas posibles para la solicitud indicada. Guarda `ruta_tentativa_id`."
          }
        },
        {
          "name": "3. OPERADOR - Confirmar ruta tentativa",
          "request": {
            "method": "POST",
            "url": {
              "raw": "{{base_url_gateway}}/logistics/routes/solicitudes/{{solicitud_id}}/rutas-tentativas/{{ruta_tentativa_id}}/confirmar",
              "host": [
                "{{base_url_gateway}}"
              ],
              "path": [
                "logistics",
                "routes",
                "solicitudes",
                "{{solicitud_id}}",
                "rutas-tentativas",
                "{{ruta_tentativa_id}}",
                "confirmar"
              ]
            },
            "description": "Persiste la ruta seleccionada y sincroniza con Orders. Guarda `ruta_id` resultante."
          }
        },
        {
          "name": "3. OPERADOR - Asignar ruta confirmada a la solicitud",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"solicitudId\": \"{{solicitud_id}}\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{base_url_gateway}}/logistics/routes/{{ruta_id}}/asignaciones",
              "host": [
                "{{base_url_gateway}}"
              ],
              "path": [
                "logistics",
                "routes",
                "{{ruta_id}}",
                "asignaciones"
              ]
            },
            "description": "Cuando se trabaja con rutas manuales, vincula la ruta con la solicitud correspondiente."
          }
        },
        {
          "name": "3. OPERADOR - Obtener ruta confirmada por solicitud",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url_gateway}}/logistics/routes/solicitudes/{{solicitud_id}}",
              "host": [
                "{{base_url_gateway}}"
              ],
              "path": [
                "logistics",
                "routes",
                "solicitudes",
                "{{solicitud_id}}"
              ]
            },
            "description": "Devuelve el resumen de la ruta ya confirmada."
          }
        },
        {
          "name": "3. OPERADOR - Listar tramos de una ruta",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url_gateway}}/logistics/routes/{{ruta_id}}/tramos",
              "host": [
                "{{base_url_gateway}}"
              ],
              "path": [
                "logistics",
                "routes",
                "{{ruta_id}}",
                "tramos"
              ]
            },
            "description": "Detalle completo de tramos planificados para coordinar asignaciones."
          }
        },
        {
          "name": "4. OPERADOR - Listar tramos operativos",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url_gateway}}/logistics/tramos",
              "host": [
                "{{base_url_gateway}}"
              ],
              "path": [
                "logistics",
                "tramos"
              ],
              "query": [
                {
                  "key": "camionId",
                  "value": "{{camion_id}}",
                  "description": "Filtrar tramos por camión asignado"
                }
              ]
            },
            "description": "Permite ver tramos por estado y camión antes de asignar nuevas cargas."
          }
        },
        {
          "name": "4. OPERADOR - Asignar camión a tramo",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"camionId\": \"{{camion_id}}\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{base_url_gateway}}/logistics/tramos/{{tramo_id}}/asignaciones",
              "host": [
                "{{base_url_gateway}}"
              ],
              "path": [
                "logistics",
                "tramos",
                "{{tramo_id}}",
                "asignaciones"
              ]
            },
            "description": "Asocia un camión disponible al tramo seleccionado."
          }
        },
        {
          "name": "4. OPERADOR - Contenedores en depósito",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url_gateway}}/logistics/tramos/deposito/{{deposito_id}}/contenedores",
              "host": [
                "{{base_url_gateway}}"
              ],
              "path": [
                "logistics",
                "tramos",
                "deposito",
                "{{deposito_id}}",
                "contenedores"
              ]
            },
            "description": "Identifica qué contenedores se encuentran esperando en el depósito indicado."
          }
        },
        {
          "name": "4. OPERADOR - Seguimiento de contenedores pendientes",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url_gateway}}/logistics/seguimiento/pendientes",
              "host": [
                "{{base_url_gateway}}"
              ],
              "path": [
                "logistics",
                "seguimiento",
                "pendientes"
              ],
              "query": [
                {
                  "key": "solicitudId",
                  "value": "{{solicitud_id}}",
                  "description": "Filtrar por solicitud"
                },
                {
                  "key": "depositoId",
                  "value": "{{deposito_id}}",
                  "description": "Filtrar por depósito"
                },
                {
                  "key": "estadoTramo",
                  "value": "EN_TRANSITO",
                  "description": "Filtrar por estado de tramo"
                }
              ]
            },
            "description": "Vista operativa para monitorear progreso global de todos los contenedores."
          }
        }
      ]
    },
